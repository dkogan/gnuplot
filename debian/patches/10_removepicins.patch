@@ -187,7 +187,6 @@ gnuplot.pdf: figures.tex pdf_figures $(s
 	  touch figures.idx ; \
 	  rm -f pdffigures.tex ; \
 	  ( echo "\usepackage{graphicx}" > pdffigures.tex ; \
-	    echo "\usepackage{picins}" >> pdffigures.tex ; \
 	  ) ; \
 	  TEXINPUTS=.:$(srcdir):$(top_srcdir):${TEXINPUTS}: $(PDFLATEX) $< ; \
